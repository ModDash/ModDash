{
  "manifest_version": 2,
  "name": "ModDash",
  "description": "Quickly switch to other open tabs with just your keyboard",
  "version": "1.0.0",
  "background": {
    "persistent": true,
    "page": "/client/src/chrome/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "./client/public/dist/index.html"
  },
  "web_accessible_resources": ["sonic-dash.gif"],
  "permissions": [
    "notifications",
    "background",
    "geolocation"
  ],
  "content_security_policy": "script-src 'self' https://cdn.pubnub.com/; object-src 'self' "
}

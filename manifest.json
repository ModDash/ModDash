{
  "manifest_version": 2,
  "name": "Widgets",
  "description": "Quickly switch to other open tabs with just your keyboard",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "RCR",
    "default_popup": "./client/src/chrome/popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "./client/src/index.html"
  },
  "permissions": [
    "notifications"
  ]
}
